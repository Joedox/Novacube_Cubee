<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="OrganicProductBase">
    <defName>NovaCube_CubeeHoney</defName>
    <label>cubee honey</label>
    <description>Cubee honey renowned for its incomparable taste bringing a little joy to anyone who consumes it.</description>
    <possessionCount>10</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/CubeeHoney</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MarketValue>10</MarketValue>
      <Mass>0.025</Mass>
      <MaxHitPoints>60</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <Nutrition>0.07</Nutrition>
      <FoodPoisonChanceFixedHuman>0.01</FoodPoisonChanceFixedHuman>
    </statBases>
    <ingestible>
      <foodType>AnimalProduct</foodType>
      <preferability>MealFine</preferability>
      <joy>0.15</joy>
      <joyKind>Gluttonous</joyKind>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
      <canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
      <babiesCanIngest>true</babiesCanIngest>
    </ingestible>
    <thingCategories>
      <li>AnimalProductRaw</li>
    </thingCategories>
    <allowedArchonexusCount>100</allowedArchonexusCount>
  </ThingDef>

  <ThingDef ParentName="EggFertBase">
    <defName>NovaCube_EggCubeeFertilized</defName>
    <label>cubee egg (fert.)</label>
    <description>Cubee egg that will give birth to a baby Cubee once it reaches maturity.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/EggCubee</texPath>
      <drawSize>1</drawSize>
    </graphicData>
    <statBases>
      <MarketValue>20.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>5</hatcherDaystoHatch>
        <hatcherPawn>Novacube_Cubee</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
</Defs>
